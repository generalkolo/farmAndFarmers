<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/statistic_nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.cleanWithCoRoutine.statistics.presentation.fragments.DashboardFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_dashboard">

        <action
            android:id="@+id/FirstFragment_to_FarmerDetailsFragment"
            app:destination="@id/FarmerDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />

    </fragment>

    <fragment
        android:id="@+id/FarmerDetailsFragment"
        android:name="com.example.cleanWithCoRoutine.statistics.presentation.fragments.FarmerDetailsFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_farmers_details">

        <action
            android:id="@+id/FarmerDetailsFragment_to_FarmDetailsFragment"
            app:destination="@id/FarmDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />

    </fragment>

    <fragment
        android:id="@+id/FarmDetailsFragment"
        android:name="com.example.cleanWithCoRoutine.statistics.presentation.fragments.FarmDetailsFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_farm_details" />

</navigation>